{
  "manifest_version": 3,
  "name": "TecConcursos Cookie Exporter",
  "description": "Exporta cookies do TecConcursos para o scraper Ouse Passar",
  "version": "1.0.1",
  "permissions": [
    "cookies",
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": [
    "https://*.tecconcursos.com.br/*",
    "*://*.tecconcursos.com.br/*",
    "http://72.61.217.225:4000/*",
    "https://mastra.ousepassar.com.br/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
