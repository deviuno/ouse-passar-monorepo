{
  "name": "ouse-passar-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:questoes": "npm run dev -w @ouse/questoes",
    "dev:site": "npm run dev -w @ouse/site",
    "build:questoes": "npm run build -w @ouse/questoes",
    "build:site": "npm run build -w @ouse/site",
    "build": "npm run build -ws",
    "typecheck:site": "npm run typecheck -w @ouse/site",
    "typecheck:questoes": "npm run typecheck -w @ouse/questoes",
    "typecheck": "npm run typecheck -ws --if-present",
    "install:all": "npm install",
    "push": "push.bat",
    "prepare": "node -e \"if (!process.env.VERCEL) require('child_process').execSync('npx husky', {stdio: 'inherit'})\""
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "husky": "^9.1.7"
  }
}
